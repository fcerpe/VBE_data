{
  "Name": "wordsDecoding MVPA",
  "BIDSModelVersion": "1.0.0",
  "Description": "default model for VisualBraille project - visualLocalizer",
  "Input": {
    "task": [
      "wordsDecoding"
    ],
    "space": [
        "IXI549Space"
    ]
  },
  "Nodes": [
    {
      "Level": "Run",
      "Name": "mvpaGLM",
      "GroupBy": [
        "run",
        "subject"
      ],
      "Transformations": {
        "Description": "merge the different semantic categories of real words into one",
        "Transformer": "cpp_spm",
        "Instructions": [
          {
            "Name": "Replace",
            "Input": "trial_type",
            "Replace": [
              {"key": "frw_liv", "value": "frw"},
              {"key": "frw_nli", "value": "frw"},
              {"key": "frw_mix", "value": "frw"},
              {"key": "brw_liv", "value": "brw"},
              {"key": "brw_nli", "value": "brw"},
              {"key": "brw_mix", "value": "brw"}
            ],
            "Attribute": "value"
          }
        ]
      },
      "Model": {
        "Type": "glm",
        "X": [
          "trial_type.frw",
          "trial_type.fpw",
          "trial_type.fnw",
          "trial_type.ffs",
          "trial_type.brw",
          "trial_type.bpw",
          "trial_type.bnw",
          "trial_type.bfs",
          "trans_x",
          "trans_y",
          "trans_z",
          "rot_x",
          "rot_y",
          "rot_z"
        ],
        "HRF": {
          "Variables": [
            "trial_type.frw",
            "trial_type.fpw",
            "trial_type.fnw",
            "trial_type.ffs",
            "trial_type.brw",
            "trial_type.bpw",
            "trial_type.bnw",
            "trial_type.bfs",
            "trial_type.target",
            "trial_type.response"
          ],
          "Model": "DoubleGamma"
        },
        "Options": {
          "HighPassFilterCutoffHz": 0.0036
        },
        "Software": {
          "SPM": {
            "SerialCorrelation": "FAST",
            "HRFderivatives": "Temporal"
          }
        }
      },
      "Contrasts": [
        {
          "Name": "frw",
          "ConditionList": [
              "trial_type.frw"
          ],
          "Weights": [
              1
          ],
          "Test": "t"
        },
        {
          "Name": "fpw",
          "ConditionList": [
              "trial_type.fpw"
          ],
          "Weights": [
              1
          ],
          "Test": "t"
        },
        {
          "Name": "fnw",
          "ConditionList": [
              "trial_type.fnw"
          ],
          "Weights": [
              1
          ],
          "Test": "t"
        },
        {
          "Name": "ffs",
          "ConditionList": [
              "trial_type.ffs"
          ],
          "Weights": [
              1
          ],
          "Test": "t"
        },
        {
          "Name": "brw",
          "ConditionList": [
              "trial_type.brw"
          ],
          "Weights": [
              1
          ],
          "Test": "t"
        },
        {
          "Name": "bpw",
          "ConditionList": [
              "trial_type.bpw"
          ],
          "Weights": [
              1
          ],
          "Test": "t"
        },
        {
          "Name": "bnw",
          "ConditionList": [
              "trial_type.bnw"
          ],
          "Weights": [
              1
          ],
          "Test": "t"
        },
        {
          "Name": "bfs",
          "ConditionList": [
              "trial_type.bfs"
          ],
          "Weights": [
              1
          ],
          "Test": "t"
        }
      ],
      "DummyContrasts": {
        "Test": "t",
        "Contrasts": [
          "trial_type.frw",
          "trial_type.fpw",
          "trial_type.fnw",
          "trial_type.ffs",
          "trial_type.brw",
          "trial_type.bpw",
          "trial_type.bnw",
          "trial_type.bfs",
          "trial_type.target",
          "trial_type.response"
        ]
      }
    },
    {
      "Level": "Subject",
      "Name": "subject_level",
      "GroupBy": [
        "contrast",
        "subject"
      ],
      "Model": {
        "X": [
          1
        ],
        "Type": "glm"
      },
      "DummyContrasts": {
        "Test": "t"
      }
    },
    {
      "Level": "Dataset",
      "Name": "dataset_level",
      "GroupBy": [
        "contrast"
      ],
      "Model": {
        "X": [
          1
        ],
        "Type": "glm"
      },
      "DummyContrasts": {
        "Test": "t"
      }
    }
  ]
}
