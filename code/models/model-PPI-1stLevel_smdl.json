{
  "Name": "PPI 1st Level Concat - GLM",
  "BIDSModelVersion": "1.0.0",
  "Description": "default model for VisualBraille project - PPI analysis",
  "Input": {
    "task": [
      "visualLocalizer"
    ]
  },
  "Nodes": [
    {
      "Level": "Run",
      "Name": "run_level",
      "GroupBy": [
        "run",
        "subject"
      ],
      "Model": {
        "Type": "glm",
        "X": [
          "trial_type.fw",
          "trial_type.sfw",
          "trial_type.bw",
          "trial_type.sbw",
          "trial_type.ld",
          "trial_type.sld",
          "trial_type.response",
          "trans_x",
          "trans_y",
          "trans_z",
          "rot_x",
          "rot_y",
          "rot_z"
        ],
        "HRF": {
          "Variables": [
            "trial_type.fw",
            "trial_type.sfw",
            "trial_type.bw",
            "trial_type.sbw",
            "trial_type.ld",
            "trial_type.sld",
            "trial_type.response"
          ],
          "Model": "DoubleGamma"
        },
        "Options": {
          "HighPassFilterCutoffHz": 0.0036
        },
        "Software": {
          "SPM": {
            "SerialCorrelation": "FAST",
            "HRFderivatives": "Temporal"
          }
        }
      },
      "Contrasts": [
        {
          "Name": "effects of interest",
          "ConditionList": [
            "trial_type.fw",
            "trial_type.sfw"
          ],
          "Weights": [
            1,
            -1
          ],
          "Test": "F"
        },
        {
          "Name": "fw",
          "ConditionList": [
            "trial_type.fw"
          ],
          "Weights": [
            1
          ],
          "Test": "t"
        },
        {
          "Name": "sfw",
          "ConditionList": [
            "trial_type.sfw"
          ],
          "Weights": [
            1
          ],
          "Test": "t"
        },
        {
          "Name": "bw",
          "ConditionList": [
            "trial_type.bw"
          ],
          "Weights": [
            1
          ],
          "Test": "t"
        },
        {
          "Name": "sbw",
          "ConditionList": [
            "trial_type.sbw"
          ],
          "Weights": [
            1
          ],
          "Test": "t"
        }
      ],
      "DummyContrasts": {
        "Test": "t",
        "Contrasts": [
          "trial_type.fw",
          "trial_type.sfw",
          "trial_type.bw",
          "trial_type.sbw",
          "trial_type.ld",
          "trial_type.sld",
          "trial_type.response"
        ]
      }
    },
    {
      "Level": "Subject",
      "Name": "subject_level",
      "GroupBy": [
        "contrast",
        "subject"
      ],
      "Model": {
        "X": [
          1
        ],
        "Type": "glm"
      },
      "DummyContrasts": {
        "Test": "t"
      }
    },
    {
      "Level": "Dataset",
      "Name": "dataset_level",
      "GroupBy": [
        "contrast"
      ],
      "Model": {
        "X": [
          1
        ],
        "Type": "glm"
      },
      "DummyContrasts": {
        "Test": "t"
      }
    }
  ]
}
