{
    "Name": "localizer GLM",
    "BIDSModelVersion": "1.0.0",
    "Description": "default model for VisualBraille project - visualLocalizer",
    "Input": {
        "task": [
            "visualLocalizer"
        ]
    },
    "Nodes": [
        {
            "Level": "Run",
            "Name": "run_level",
            "Model": {
                "Type": "glm",
                "X": [
                    "trial_type.fw",
                    "trial_type.sfw",
                    "trial_type.bw",
                    "trial_type.sbw",
                    "trial_type.ld",
                    "trial_type.sld",
                    "trans_x",
                    "trans_y",
                    "trans_z",
                    "rot_x",
                    "rot_y",
                    "rot_z"
                ],
                "HRF": {
                    "Variables": [
                      "trial_type.fw",
                      "trial_type.sfw",
                      "trial_type.bw",
                      "trial_type.sbw",
                      "trial_type.ld",
                      "trial_type.sld"
                    ],
                    "Model": "DoubleGamma"
                },
                "Options": {
                    "HighPassFilterCutoffHz": 0.0078,
                    "Mask": ""
                },
                "Software": {
                    "SPM": {
                        "SerialCorrelation": "FAST",
                        "HRFderivatives": "Temporal"
                    }
                }
            },
            "DummyContrasts": {
                "Test": "t",
                "Contrasts": [
                  "trial_type.fw",
                  "trial_type.sfw",
                  "trial_type.bw",
                  "trial_type.sbw",
                  "trial_type.ld",
                  "trial_type.sld"
                ]
            }
        },
        {
            "Level": "Subject",
            "Name": "subject_level",
            "Model": {
                "X": [
                    "trial_type.fw",
                    "trial_type.sfw",
                    "trial_type.bw",
                    "trial_type.sbw",
                    "trial_type.ld",
                    "trial_type.sld",
                    "trans_x",
                    "trans_y",
                    "trans_z",
                    "rot_x",
                    "rot_y",
                    "rot_z"
                ],
                "HRF": {
                    "Variables": [
                        "trial_type.fw",
                        "trial_type.sfw",
                        "trial_type.bw",
                        "trial_type.sbw",
                        "trial_type.ld",
                        "trial_type.sld"
                    ],
                    "Model": "DoubleGamma"
                },
                "Type": "glm",
                "Options": {
                    "HighPassFilterCutoffHz": 0.0078,
                    "Mask": ""
                },
                "Software": {
                    "SPM": {
                        "SerialCorrelation": "FAST",
                        "HRFderivatives": "Temporal"
                    }
                }
            },
            "Contrasts": [
                {
                    "Name": "french_gt_scrambled",
                    "ConditionList": [
                        "trial_type.fw",
                        "trial_type.sfw"
                    ],
                    "Weights": [
                        1,
                        -1
                    ],
                    "Type": "t"
                },
                {
                    "Name": "braille_gt_scrambled",
                    "ConditionList": [
                        "trial_type.bw",
                        "trial_type.sbw"
                    ],
                    "Weights": [
                        1,
                        -1
                    ],
                    "Type": "t"
                },
                {
                    "Name": "drawing_gt_scrambled",
                    "ConditionList": [
                        "trial_type.ld",
                        "trial_type.sld"
                    ],
                    "Weights": [
                        1,
                        -1
                    ],
                    "Type": "t"
                },
                {
                    "Name": "frWords_gt_scrLines",
                    "ConditionList": [
                        "trial_type.fw",
                        "trial_type.sld"
                    ],
                    "Weights": [
                        1,
                        -1
                    ],
                    "Type": "t"
                },
                {
                    "Name": "allWords_gt_scrLines",
                    "ConditionList": [
                        "trial_type.fw",
                        "trial_type.bw",
                        "trial_type.sld"
                    ],
                    "Weights": [
                        0.5,
                        0.5,
                        -1
                    ],
                    "Type": "t"
                }
            ],
            "DummyContrasts": {
                "Test": "t",
                "Contrasts": [
                    "trial_type.fw",
                    "trial_type.sfw",
                    "trial_type.bw",
                    "trial_type.sbw",
                    "trial_type.ld",
                    "trial_type.sld"
                ]
            }
        },
        {
            "Level": "Dataset",
            "Name": "dataset_level",
            "Model": {
                "X": [
                    " "
                ],
                "Type": "glm",
                "Options": {
                    "Mask": ""
                }
            },
            "DummyContrasts": {
                "Test": "t",
                "Contrasts": [
                    "trial_type.fw",
                    "trial_type.sfw",
                    "trial_type.bw",
                    "trial_type.sbw",
                    "trial_type.ld",
                    "trial_type.sld"
                ]
            }
        }
    ]
}
